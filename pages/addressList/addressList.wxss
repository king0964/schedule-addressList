.addressList-title {
  background-color: #efeff4;
  padding: 15rpx 25rpx;
  color: #888;
}
.addressList-name-box {
  background: #fff;
  border-bottom: 2rpx solid #e5e5e5;
  display: flex;
  align-items: center;
  padding: 30rpx 25rpx;
  font-size: 34rpx;
}
.addressList-item-name {
  flex: 1;
}
.icon-down {
  width: 31rpx;
  height: 20rpx;
  transition: transform 0.3s;
  padding: 12rpx 20rpx;
}
.addressList-item-name-box {
  color: #4b4b4c;
  display: flex;
  align-items: center;
  padding: 30rpx 25rpx 30rpx 50rpx;
  border-bottom: 2rpx solid #e5e5e5;
  font-size: 34rpx;
}
.other {
  display: flex;
  padding: 30rpx 25rpx 30rpx 75rpx;
  align-items: center;
  color: #666;
  border-bottom: 2rpx solid #e5e5e5;
  font-size: 34rpx;
  background-color: #fafafa;
}
.icon-down-rotate {
  transform: rotate(180deg);
}
/* 选人样式 */
.addressList-selected {
  position: fixed;
  width: 100%;
  background-color: #f5f5fa;
  padding: 25rpx 25rpx 0 25rpx;
  box-sizing: border-box;
}
.addressList-selected-title {
  color: #888;
  margin-bottom: 24rpx;
}
.addressList-selected-title image {
  float: right;
  /* vertical-align: middle;
  margin-top: 10rpx; */
}
.addressList-selected-item {
  display: inline-block;
  background-color: #e0eaf9;
  border-radius: 26rpx;
  height: 52rpx;
  line-height: 52rpx;
  padding: 0 12rpx;
  color: #4b6a96;
  margin:0 38rpx 25rpx 0;
}
.addressList-selected-item.last {
  margin-right: 0;
}
.addressList-selected-item image {
  width: 22rpx;
  height: 22rpx;
  vertical-align: middle;
  margin: -2rpx 0 0 16rpx;
}
